  "source": "pragma solidity ^0.4.24;\n\nimport \"./Ballotable.sol\";\n\n\ncontract BallotableContent is Ballotable {\n    address public creator;\n    uint public contentID;\n    uint public createdAt;\n    uint public updatedAt;\n\n    constructor(\n        address _creator,\n        uint _contentID\n    ) public {\n        creator = _creator;\n        contentID = _contentID;\n        createdAt = now;\n        updatedAt = now;\n    }\n}",
